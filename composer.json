{
    "name": "lidai/laravel-imageup",
    "description": "Auto Image upload, resize and crop for Laravel eloquent model using Intervention image",
    "homepage": "https://github.com/lidaijfm/laravel-imageup",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "image upload",
        "image crop",
        "image resize",
        "intervention image",
        "eloquent",
        "model"
    ],
    "authors": [
        {
            "name": "Liam Aird",
            "email": "lidai@jfmedier.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
        "dms/phpunit-arraysubset-asserts": "^0.2.0|^0.3|^0.4|^0.5"
    },
    "autoload": {
        "psr-4": {
            "Lidai\\ImageUp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lidai\\ImageUp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lidai\\ImageUp\\ImageUpServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
